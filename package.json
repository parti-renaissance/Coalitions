    "source-map-explorer": "^2.5.2",
    "styled-components": "4.4.1",
    "superagent": "5.3.1"
  },
  "scripts": {
    "audit-dependencies": "check-audit --yarn",
    "start": "REACT_APP_ENV=local react-app-rewired start",
    "start-server": "env-cmd .env.${REACT_APP_ENV} ts-node --project tsconfig.server.json --require tsconfig-paths/register server/index.js",
    "build": "env-cmd .env.${REACT_APP_ENV} react-app-rewired build",
    "test": "react-app-rewired test --maxWorkers=4",
    "eject": "react-app-rewired eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test:coverage": "npm run test -- --coverage",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --max-warnings 50",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint-style": "stylelint 'src/**/*.style.{ts,tsx}' 'src/**/style.{ts,tsx}'",
    "type-check": "tsc"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/actions.js",
      "!src/**/index.js",
      "!src/**/*.wrap.js",
      "!src/index.js",
      "!src/tempPolyfills.js",
      "!src/setupTests.js",
      "!src/redux/reducers.js",
      "!src/redux/sagas.js",
      "!src/redux/store.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "215 kB"
    }
  ],
  "devDependencies": {
    "@babel/register": "^7.13.16",
    "@types/cheerio": "0.22.24",
    "@types/draft-convert": "^2.1.2",
    "@types/enzyme": "3.10.8",
    "@types/history": "4.7.8",
    "@types/jest": "24.9.1",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash": "4.14.168",
    "@types/node": "10.17.54",
    "@types/react": "16.14.4",
    "@types/react-dom": "16.9.11",
    "@types/react-helmet": "^6.1.1",
    "@types/react-intl": "3.0.0",
    "@types/react-redux": "7.1.16",
    "@types/react-router": "5.1.11",
    "@types/react-router-dom": "5.1.7",
    "@types/react-test-renderer": "16.9.5",
    "@types/styled-components": "4.4.3",
    "@types/superagent": "4.1.10",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "babel-plugin-styled-components": "1.12.0",
    "babel-plugin-transform-assets": "^1.0.2",
    "bundlesize": "0.18.1",
    "cypress": "3.8.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-mysticatea": "4.2.4",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-risxss": "1.3.0",
    "html-webpack-inject-attributes-plugin": "^1.0.6",
    "npm-audit-resolver": "2.2.0",
    "plop": "2.7.4",
    "prettier": "1.19.1",
    "react-app-rewire-webpack-bundle-analyzer": "1.1.0",
    "react-app-rewired": "2.1.8",
    "react-test-renderer": "16.14.0",
    "redux-saga-test-plan": "4.0.0-rc.3",
    "stylelint": "12.0.1",
    "stylelint-config-prettier": "7.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-declaration-strict-value": "1.1.8",
    "stylelint-processor-styled-components": "1.10.0",
